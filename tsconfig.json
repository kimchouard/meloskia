{
  "compilerOptions": {
    "baseUrl": ".",
    "moduleSuffixes": [".ios", ".android", ".native", ".web", ""],
    "paths": {
      "@/*": [
        "./src/*"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "types": ["@webgpu/types"]
  },
  "extends": "expo/tsconfig.base",
  "include": [
    "global.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "tailwind.config.js",
    "nativewind-env.d.ts"
  ]
}